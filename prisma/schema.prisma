datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

generator client {
  provider = "prisma-client-js"
}

model Race {
  id        Int      @id @default(autoincrement())
  circuit   String
  cars      String
  maxir     Int

  @@map("races")
}
